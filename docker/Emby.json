{
  "name": "Emby",
  "repo": "emby/embyserver:latest",
  "registry": "https://registry.hub.docker.com/u/emby/embyserver/",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "bash",
  "privileged": false,
  "extra_parameters": "--restart unless-stopped",
  "post_parameters": null,
  "cpu_set": null,
  "web_ui_url": "http://[IP]:[PORT:8096]/",
  "icon": "https://raw.githubusercontent.com/MediaBrowser/Emby.Resources/master/images/Logos/logoicon.png",
  "category": "MediaServer:Video MediaServer:Music MediaServer:Photos",
  "project": "https://emby.media/",
  "support": "https://emby.media/community/index.php",
  "description": "Emby is a personal media server that allows users to organize, present, and stream their own videos, music, photos, and live TV to various devices on a home network or over the internet.",
  "donate": null,
  "requires": null,
  "paths": [
    {
      "name": "Host Path 2",
      "host": "/mnt/cache/Movies/",
      "container": "/mnt/Movies"
    },
    {
      "name": "Serien",
      "host": "/mnt/cache/TV/",
      "container": "/mnt/TV"
    },
    {
      "name": "AppData Config Path",
      "host": "/mnt/cache/appdata/Emby/",
      "container": "/config"
    }
  ],
  "ports": [
    {
      "name": "Host Port 1",
      "protocol": "tcp",
      "host": "8096",
      "container": "8096"
    }
  ],
  "variables": [
    {
      "name": "Key 1",
      "key": "UID",
      "value": "2",
      "mask": false
    },
    {
      "name": "Key 2",
      "key": "GID",
      "value": "2",
      "mask": false
    },
    {
      "name": "Key 3",
      "key": "GIDLIST",
      "value": "2",
      "mask": false
    }
  ],
  "devices": [],
  "labels": [],
  "gpus": []
}
