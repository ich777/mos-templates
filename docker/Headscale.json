{
  "name": "Headscale",
  "repo": "ghcr.io/juanfont/headscale:v0.27.0",
  "registry": "https://github.com/juanfont/headscale/pkgs/container/headscale",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": "--no-healthcheck --restart=unless-stopped --log-driver=none",
  "post_parameters": "serve",
  "cpu_set": null,
  "web_ui_url": null,
  "icon": "https://raw.githubusercontent.com/ich777/docker-templates/master/ich777/images/headscale.png",
  "category": [ "web" ],
  "project": "https://github.com/juanfont/headscale",
  "support": "https://github.com/juanfont/headscale/issues",
  "description": "Headscale is an open-source, self-hosted implementation of the Tailscale control server, which allows users to run their own private network, or \"tailnet\", without relying on Tailscale's cloud infrastructure. ",
  "donate": "https://ko-fi.com/headscale",
  "requires": null,
  "paths": [
    {
      "name": "Host Path 1",
      "host": "/mnt/cache/appdata/headscale/config",
      "container": "/etc/headscale"
    },
    {
      "name": "Host Path 2",
      "host": "/mnt/cache/appdata/headscale/data",
      "container": "/var/lib/headscale"
    }
  ],
  "ports": [],
  "variables": [],
  "devices": [],
  "labels": [],
  "gpus": []
}
